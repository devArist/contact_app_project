<ContactBoxLayout>:
    orientation: 'vertical'
    MDToolbar:
        title: 'contacts'
        anchor_title: 'center'
        left_action_items: [["menu"]]
        right_action_items: [["menu"]]
    MDRelativeLayout:
        ScrollView:
            # canvas.before:
            #     Color:
            #         rgb: 0,0,1
            #     Rectangle:
            #         pos:self.pos
            #         size:self.size
            pos_hint: {'top':1}
            MDList:
                OneLineAvatarListItem:
                    text: 'Joule'
                    on_release: app.manager.push('detail')
                    IconLeftWidget:
                        icon:'github'
                OneLineAvatarListItem:
                    text: 'Prescott'
                    on_release: app.manager.push('detail')
                    IconLeftWidget:
                        icon:'github'
        MDFloatingActionButtonSpeedDial:
            size_hint: None,None
            data: app.data
            rotation_root_button: True